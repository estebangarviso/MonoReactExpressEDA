{
  "$schema": "../node_modules/@v10e/env/schemas/env.schema.json",
  "|DEFAULT|": {
    "APP_ENV": "[[ENV]]",
    "APP_NAME": "[[NAME]]",
    "APP_VERSION": "[[VERSION]]",
    "TZ": "America/Santiago",
    "LANG": "es-CL.UTF-8",
    "SWAGGER_UI": true,
    "BASE_URL": "api/",
    "SECURITY": {
      "ENABLED": false,
      "HEADER_NAME": "x-api-key"
    },
    "LOG_LEVEL": "info",
    "PRODUCER_APP_NAME": "@demo/api-producer"
  },
  "|MODE|": {
    "build": {
      "NODE_ENV": "production",
      "PORT": 8081
    },
    "debug": {
      "NODE_ENV": "development",
      "PORT": 4005,
      "BASE_URL": "api",
      "SECURITY": {
        "API_KEY": "debug"
      },
      "LOG_LEVEL": "debug"
    },
    "test": {
      "NODE_ENV": "test"
    }
  },
  "|ENV|": {
    "dev": {
      "REDIS_URI": "redis://0.0.0.0:6379",
      "RABBITMQ_URI": "amqp://rabbitmq_user:rabbitmq_pass@0.0.0.0:5672/",
      "ALLOWED_ORIGINS": "http://0.0.0.0:3003",
      "SALT_ROUNDS": 10,
      "JWT_SECRET": "secret",
      "LOG_LEVEL": "debug"
    },
    "release": {
      "REDIS_URI": "redis://0.0.0.0:6379",
      "RABBITMQ_URI": "amqp://rabbitmq_user:rabbitmq_pass@0.0.0.0:5672/",
      "ALLOWED_ORIGINS": "http://0.0.0.0:3003",
      "SALT_ROUNDS": 11,
      "JWT_SECRET": "supersecret",
      "LOG_LEVEL": "debug"
    }
  },
  "|LOCAL|": {
    "dev": {},
    "release": {}
  }
}
